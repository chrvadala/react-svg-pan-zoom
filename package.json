{
  "author": "chrvadala",
  "name": "react-svg-pan-zoom",
  "version": "2.7.0",
  "description": "A React component that adds pan and zoom features to SVG",
  "main": "./build-commonjs/index.js",
  "module": "./build-es/index.js",
  "browser": "./build-umd/react-svg-pan-zoom.min.js",
  "jsnext:main": "./build-es/index.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "cross-env BABEL_ENV=commonjs start-storybook -p 9001 -s storybook/public -c storybook",
    "build-commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build-commonjs",
    "build-es": "cross-env BABEL_ENV=es babel src --out-dir build-es",
    "build-umd-min": "cross-env BABEL_ENV=es webpack --env.minimize",
    "build-umd": "cross-env BABEL_ENV=es webpack",
    "build-storybook": "cross-env BABEL_ENV=commonjs build-storybook -s storybook/public -c storybook -o build-storybook",
    "build": "npm-run-all clean build-storybook build-commonjs build-es build-umd build-umd-min",
    "clean": "del build-*",
    "website-update": "gh-pages -m 'Update gh-pages' -d ./build-storybook",
    "version": "npm run build",
    "postpublish": "npm run website-update",
    "test": "mocha --compilers js:babel-core/register ./src/**/*.spec.js"
  },
  "files": [
    "*.md",
    "build-*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrvadala/react-svg-pan-zoom.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrvadala/react-svg-pan-zoom/issues"
  },
  "homepage": "https://github.com/chrvadala/react-svg-pan-zoom#readme",
  "keywords": [
    "react",
    "react-component",
    "svg",
    "pan",
    "zoom",
    "react",
    "ui",
    "user",
    "interface",
    "component",
    "web-components"
  ],
  "dependencies": {
    "prop-types": "^15.5.0",
    "transformation-matrix": "^1.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-actions": "^1.1.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-webpack2": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "del-cli": "^0.2.1",
    "gh-pages": "^0.12.0",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-virtualized": "^9.7.5",
    "remarkable": "^1.7.1",
    "raw-loader": "^0.5.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  }
}
